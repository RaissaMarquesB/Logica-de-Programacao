algoritmo "semnome"
// Função :
// Autor :
// Data : 20/04/2021
// Seção de Declarações 
var
c, n, f: inteiro
inicio
      escreva("Digite um numero: ")
      leia(n)
      c <- n
      f <- 1
      repita
            f <- f*C
            c<- c - 1
      ate (c<1)
      escreva("o valor do fatorial de ",n," é igual a", f)


fimalgoritmo
